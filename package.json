{
  "private": true,
  "name": "cut-the-line",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "per-env",
    "start:production": "npm run -s serve",
    "start:development": "npm run -s dev",
    "build": "preact build --no-prerender",
    "serve": "preact build --no-prerender && sirv build --cors --single",
    "buildVendor": "preact build --no-prerender --src src/vendor --dest buildVendor",
    "serveVendor": "preact build --no-prerender --src src/vendor --dest buildVendor && sirv buildVendor --cors --single --port 5001",
    "dev": "preact watch",
    "lint": "eslint src",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "build/"
    ]
  },
  "devDependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-preact-pure": "^2.0.0",
    "eslint": "^6.0.1",
    "eslint-config-preact": "^1.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-preset-preact": "^1.0.0",
    "per-env": "^1.0.2",
    "preact-cli": "^3.0.0-rc.6",
    "preact-render-spy": "^1.2.1",
    "sirv-cli": "^0.4.5"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "graphql.js": "^0.6.1",
    "netlify-lambda": "^1.6.3",
    "preact": "^10.4.0",
    "preact-i18n": "^2.0.0-preactx.2",
    "preact-render-to-string": "^5.1.4",
    "preact-router": "^3.2.1",
    "pusher": "^3.0.1",
    "pusher-js": "^5.1.1",
    "query-string": "^6.11.1",
    "redux-zero": "^5.1.3",
    "tachyons": "^4.11.1"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js",
      "<rootDir>/tests/__mocks__/setupTests.js"
    ]
  }
}
